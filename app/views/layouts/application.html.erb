<!DOCTYPE html>
<html>
  <head>
    <% if content_for?(:title) %>
    <title><%= content_for(:title) %></title>
    <% else %>
    <title>VLife</title>
    <% end %>
      
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' ,:cache => true%>
    <%= javascript_include_tag 'application' , 'data-turbolinks-track': 'reload' unless current_user.nil? %>
    <meta name="turbolinks-cache-control" content="no-cache">    
  </head>

  <body>
  	<%= render '/layouts/nav' %>
    <div class="container">
    <%= yield %>
    </div>
  </body>
</html>
